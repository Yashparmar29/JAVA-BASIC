import java.util.Scanner;
public class pattern8 {
     public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int no1 = sc.nextInt();
        int j,i;
        int number = 1;

        for(i=1 ; i<=no1 ;i++)
        {
            for(j=1 ; j<=i; j++)
            {
                System.out.print(number);
                number++;
            }
            System.out.println();
        }
    }
    
}
