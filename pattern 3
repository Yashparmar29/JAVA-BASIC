import java.util.Scanner;

public class pattern2
{
    public static void main(String[] args) 
    {
        Scanner sc = new Scanner(System.in);
        int no = sc.nextInt();
        int no1 = sc.nextInt();
        int i , j ;
        
        for(i=1;i<=no;i++)
        {
            for(j=1;j<no1;j++)
            {
                System.out.print("*");
            }
            System.out.println("*");
        }

    }
}
