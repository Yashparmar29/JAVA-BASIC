// Write a program to calculate and print the first n Fibonacci numbers. (Accept n from command line)
class logic
{
    int no;
    logic(int no)
    {
        this.no = no;
    }
    void count()
    {
        int n1 = 0;
        int n2 = 1;
        System.out.print(n1 + "," + n2 + ",");
        
        for(int i=2;i<no;i++)
        {
            int n3 = n1 + n2;
            System.out.print(n3 + ",");
            n1 = n2;
            n2 = n3;
        }
    }
}
public class fibbonacci_series 
{
    public static void main(String[] args)
    {
        int no = Integer.parseInt(args[0]);
        logic l = new logic(no);
        l.count();
    }    
}
