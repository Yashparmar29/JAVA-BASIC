class Table
{
    synchronized void printTable(int n)
    {
        for(int i = 1; i <= 5; i++)
        {
            System.out.println(n * i);
            try {
                Thread.sleep(500);
            } catch(Exception e) {
                System.out.println(e);
            }
        }
    }
}

class MyThread implements Runnable
{
    Table t;
    int num;

    MyThread(Table t, int num)
    {
        this.t = t;
        this.num = num;
    }

    public void run()
    {
        t.printTable(num);
    }
}

public class SyncRunnableExample
{
    public static void main(String[] args)
    {
        Table obj = new Table();  

        Thread t1 = new Thread(new MyThread(obj, 5));
        Thread t2 = new Thread(new MyThread(obj, 10));

        t1.start();
        t2.start();
    }
}
