import java.util.Scanner;
public class even_or_odd 
{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        System.out.print("enter number of limit :- ");
        int a = sc.nextInt();
        System.out.print("Even number :- ");
        for(int i=1;i<=a;i++)
        {
            if((i%2)==0)
            {
                System.out.print(i + " ");
            }
        }
    }    
}

