import java.util.Scanner;
public class pattern5 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int no1 = sc.nextInt();
        int j,i,n;

        for(i=1 ; i<=no1 ;i++)
        {
            for(n=i ; n<=no1-1 ; n++)
            {
                System.out.print(" ");
            }
            for(j=1;j<=i;j++)
            {
                System.out.print("*");
            }
           System.out.println("");
        }
    }
}
